{
  "name": "casperlabs-solana-bridge",
  "version": "1.0.0",
  "description": "",
  "scripts": {
    "deploy:w_pokt": "solana program deploy build/w_pokt.so",
    "start": "npm run deploy:w_pokt && ts-node src/client/main.ts",
    "start-with-test-validator": "start-server-and-test 'solana-test-validator --reset --quiet' http://localhost:8899/health start",
    "build:program": "cargo build-bpf --manifest-path=./src/program/Cargo.toml --bpf-out-dir=./build",
    "clean:program": "cargo clean --manifest-path=./src/program/Cargo.toml && rm -rf ./build",
    "test:program": "cargo test-bpf --manifest-path=./src/program/Cargo.toml",
    "fmt:program": "cargo fmt --manifest-path=./src/program/Cargo.toml"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Scytalelabs-official/CasperLabs-Solana-bridge.git"
  },
  "author": "Muhammad Talha Dar",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/Scytalelabs-official/CasperLabs-Solana-bridge/issues"
  },
  "homepage": "https://github.com/Scytalelabs-official/CasperLabs-Solana-bridge#readme",
  "devDependencies": {
    "@solana/buffer-layout": "^4.0.0",
    "@solana/spl-token": "^0.2.0",
    "@solana/web3.js": "^1.39.1",
    "@tsconfig/recommended": "^1.0.1",
    "@types/eslint": "^8.2.2",
    "@types/eslint-plugin-prettier": "^3.1.0",
    "@types/mz": "^2.7.2",
    "@types/prettier": "^2.1.5",
    "@types/yaml": "^1.9.7",
    "@typescript-eslint/eslint-plugin": "^5.20.0",
    "@typescript-eslint/parser": "^5.20.0",
    "bn.js": "^5.2.0",
    "borsh": "^0.7.0",
    "buffer": "^6.0.3",
    "eslint": "^8.13.0",
    "eslint-config-google": "^0.14.0",
    "eslint-config-prettier": "^6.15.0",
    "eslint-plugin-prettier": "^4.0.0",
    "mz": "^2.7.0",
    "prettier": "^2.6.2",
    "start-server-and-test": "^1.11.6",
    "ts-node": "^10.0.0",
    "typescript": "^4.0.5",
    "yaml": "^2.0.1"
  },
  "engines": {
    "node": ">=14.0.0"
  }
}
